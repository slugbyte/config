set preview true 
set color256 true
set ignorecase true
set hidden true
set number true
set relativenumber true

map n down
map e up
map y updir
map o open
map k copy
map <c-p> redraw
map t find
map T find-back
map h find-next
map H find-prev
map i $$EDITOR $f
map f search-next
map F search-prev
map m mark-save
map M mark-load
map s shell
map a shell-async
map w shell-wait
